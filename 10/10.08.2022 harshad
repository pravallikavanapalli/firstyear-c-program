#include <stdio.h>

int main() {
    int n,sum=0,r;
    scanf("%d",&n);
    while(n)
    {
        r=n%10;
        sum=sum+r;
        n=n/10;
    }
    if(n/sum==0)
    printf("Harshad number");
    else
    printf("Not harshad number");

    return 0;
}
OUTPUT:156
Harshad number
OUTPUT:2
22
Not harsha number
